{
    "name": "snowflake-vscode",
    "displayName": "Snowflake VSCode",
    "description": "This extension will highlight keywords and functions for the [Snowflake](https://www.snowflake.com/) flavour of SQL in VSCode.",
    "version": "0.0.1",
    "author": {
        "name": "Daniel O'Keeffe",
        "email": "okeeffdp@tcd.ie"
    },
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "assets/snowflake-icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/okeeffdp/snowflake-vscode.git"
    },
    "homepage": "https://github.com/okeeffdp/snowflake-vscode/blob/main/README.md",
    "keywords": [
        "Snowflake SQL",
        "Snowflake",
        "SQL",
        "Snowflake Data Warehouse",
        "Data Warehouse"
    ],
    "contributes": {
        "languages": [
            {
                "id": "snowflake",
                "aliases": [
                    "Snowflake",
                    "snowflake"
                ],
                "extensions": [
                    ".snowql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "snowflake",
                "scopeName": "source.snowflake",
                "path": "./syntaxes/snowflake.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
